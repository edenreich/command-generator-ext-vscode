{
	"name": "cpp-command-generator",
	"displayName": "cpp-command-generator",
	"description": "An helper for generating commands",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:plaintext",
		"onLanguage:cpp"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.makeCommand",
				"title": "cpp: generate a new command"
			},
			{
				"command": "extension.makeProject",
				"title": "cpp: generate a new console project"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1"
	}
}
